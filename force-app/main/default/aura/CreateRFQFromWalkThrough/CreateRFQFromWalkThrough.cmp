<aura:component controller="CreateRFQFromWalkThroughController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
	<!-- <aura:attribute name="recordId" type="Id"/> -->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <lightning:notificationsLibrary aura:id="notifLib"/>
    <div aura:id="progressbar">
    	<center>
	    	<img alt="Creating a new RFQ record." src="{!$Resource.gearsloading}" style="height:150px !important;"/>
	    	<br/>
	    	Creating a new RFQ <br/> Please don't leave this page until RFQ is created.
    	</center> 
    </div>
</aura:component>