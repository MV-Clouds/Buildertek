<aura:component implements="flexipage:availableForAllPageTypes,lightning:isUrlAddressable" access="global">
    <aura:attribute type="String" name="budgetId"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>    
    <lightning:workspaceAPI aura:id="workspace" />

    <div class="slds-card">
        <c:massUpdateOnBudget budgetId="{!v.budgetId}" oncancel="{!c.handleCancelEvent}"/>
    </div>
</aura:component>
